{
    "manifest_version": 3,
    "name": "Fullscreen video",
    "version": "2.0",
    "description": "Press \"F\" to enter/close Fullscreen mode",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "128": "images/icon-128.png"
      },
    "content_scripts": [
    {
        "js": ["scripts/content.js"],
        "matches": [
          "<all_urls>" 
        ]
    }
    ]

  }